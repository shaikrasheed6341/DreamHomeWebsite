// schema.prisma


generator client {
  provider = "prisma-client-js"
  output   = "./generated/client"
}
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  firstname      String
  lastname      String
  email     String   @unique
  password String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}




